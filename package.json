{
  "name": "unique_tslib",
  "version": "0.0.1",
  "sideEffects": false,
  "license": "SEE LICENSE IN License",
  "exports": {
    ".": {
      "import": "./lib/bundle.min.mjs",
      "require": "./lib/bundle.min.cjs"
    }
  },
  "main": "./lib/bundle.min.cjs",
  "module": "./lib/bundle.min.mjs",
  "source": "./src/index.ts",
  "types": "./lib/bundle.d.ts",
  "files": [
    "lib/",
    "src/"
  ],
  "scripts": {
    "clean": "rimraf lib/*",
    "ts": "yarn clean && tsc --project tsconfig.json",
    "build": "yarn clean && rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript --sourcemap",
    "develop": "yarn build --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/plugin-transform-runtime": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@rollup/plugin-typescript": "^8.3.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.71.1",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^2.0.7",
    "rollup-plugin-typescript2": "^0.31.2",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@polkadot/api": "^8.3.1",
    "@polkadot/util-crypto": "^9.1.1",
    "@unique-nft/types": "0.4.0",
    "cjstoesm": "^1.1.4",
    "web3": "^1.7.3"
  }
}
