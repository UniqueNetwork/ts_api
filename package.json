{
  "name": "unique_tslib",
  "version": "0.0.1",
  "sideEffects": false,
  "license": "SEE LICENSE IN License",
  "main": "./lib/bundle.cjs",
  "module": "./lib/bundle.js",
  "source": "./src/index.ts",
  "types": "./lib/bundle.d.ts",
  "files": [
    "lib/",
    "src/"
  ],
  "scripts": {
    "ts": "tsc --project tsconfig.json",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo && mkdir lib",
    "build": "yarn clean && yarn ts",
    "rollup": "rollup --config rollup.config.ts --configPlugin typescript --sourcemap",
    "analyze": "yarn rollup --analyze",
    "develop": "yarn rollup --watch",
    "test": "vitest"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/node": "^17.0.35",
    "rimraf": "^3.0.2",
    "rollup": "^2.71.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^2.0.7",
    "rollup-plugin-typescript2": "^0.31.2",
    "typescript": "^4.6.4",
    "vitest": "^0.12.6"
  },
  "dependencies": {
    "@polkadot/api": "8.6.2",
    "@polkadot/extension-dapp": "0.43.2",
    "@polkadot/keyring": "9.3.1",
    "@polkadot/util": "9.3.1",
    "@polkadot/util-crypto": "9.3.1",
    "@unique-nft/types": "0.5.1",
    "ethers": "5.6.8"
  }
}
